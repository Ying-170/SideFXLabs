--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/select_edge_loop.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int max_iteration = chi(\"max_iteration\");

// When 'max_iteration' is zero, the start edges will not
// be selected. If the start edges are always selected, then
// we can't correctly offset the selection pattern.
if (max_iteration < 1) {
    return;
}

int iteration = 0;
int start_pt0 = point(1, \"_lfs_index\", 0);
int start_pt1 = point(1, \"_lfs_index\", 1);

string group_name = chs(\"group_name\");
int create_group_per_loop = chi(\"create_group_per_loop\");
int loop_index = point(1, \"_lfs_loop_index\", 0);

int apply_pattern = chi(\"apply_pattern\");
int pattern_offset, select_length, skip_length, period;

// Selects the start edges according to the pattern
if (!apply_pattern) {

    setedgegroup(0, group_name, start_pt0, start_pt1, 1);

    if (create_group_per_loop) {
        setedgegroup(0, group_name + itoa(loop_index), start_pt0, start_pt1, 1);
    }

} else {

    pattern_offset = chi(\"pattern_offset\");
    select_length = chi(\"select_length\");
    skip_length = chi(\"skip_length\");
    period = select_length + skip_length;

    if ((iteration + pattern_offset) % period < select_length) {

        setedgegroup(0, group_name, start_pt0, start_pt1, 1);

        if (create_group_per_loop) {
            setedgegroup(0, group_name + itoa(loop_index), start_pt0, start_pt1, 1);
        }
    }
}

if (max_iteration == 1) {
    return;
}

max_iteration --;

// The two search directions are identified as a and b.
int a_prev_pt = start_pt0;
int a_this_pt = start_pt1;
int a_next_pt = -1;
int b_prev_pt = start_pt1;
int b_this_pt = start_pt0;
int b_next_pt = -1;

// Should keep going: 0
// The loop is complete: 1
// There is no unique valid next point: 2
// The max iteration is reached: 10
// There is no face attached to this edge: -1
// The mesh is non-manifold at this edge: -3
int a_termination_code = 0;
int b_termination_code = 0;

// Attempts to complete the loop from both directions
while (a_termination_code == 0 || b_termination_code == 0) {

    iteration ++;

    if (a_termination_code == 0) {

        // Looks for a valid hedge corresponding to this edge

        int hedge = pointhedge(0, a_prev_pt, a_this_pt);

        if (hedge == -1) {

            hedge = pointhedge(0, a_this_pt, a_prev_pt);

            if (hedge == -1) {
                error(\"There is no edge between points %d and %d.\", a_prev_pt, a_this_pt);
                return;
            }
        }

        // Looks for a unique valid next edge to potentially select

        int hedge_equivcount = hedge_equivcount(0, hedge);

        if (hedge_equivcount < 3) {

            if (hedge_equivcount == 1) {
                if (hedge_presrcpoint(0, hedge) == -1) {
                    a_termination_code = -1;
                }
            }

            if (a_termination_code == 0) {

                // Builds the invalid point array

                int invalid_pts[] = {};
                append(invalid_pts, a_prev_pt);

                for (int i = 0; i < hedge_equivcount; i ++) {
                    append(invalid_pts, hedge_presrcpoint(0, hedge));
                    append(invalid_pts, hedge_postdstpoint(0, hedge));
                    hedge = hedge_nextequiv(0, hedge);
                }

                // Looks for valid next points from neighbour points

                int neighbour_pts[] = neighbours(0, a_this_pt);
                int count = 0;

                foreach (int neighbour_pt; neighbour_pts) {

                    if (find(invalid_pts, neighbour_pt) < 0) {
                        a_next_pt = neighbour_pt;
                        count ++;
                    }
                }

                // If this is a unique valid next point, selects
                // the corresponding edge according to the pattern

                if (count == 1) {

                    if (!apply_pattern) {

                        setedgegroup(0, group_name, a_this_pt, a_next_pt, 1);

                        if (create_group_per_loop) {
                            setedgegroup(0, group_name + itoa(loop_index), a_this_pt, a_next_pt, 1);
                        }

                    } else {

                        if ((iteration + pattern_offset) % period < select_length) {

                            setedgegroup(0, group_name, a_this_pt, a_next_pt, 1);

                            if (create_group_per_loop) {
                                setedgegroup(0, group_name + itoa(loop_index), a_this_pt, a_next_pt, 1);
                            }
                        }
                    }

                    a_prev_pt = a_this_pt;
                    a_this_pt = a_next_pt;

                    if (a_this_pt == b_this_pt) {
                        a_termination_code = 1;
                        b_termination_code = 1;
                    }

                } else {

                    a_termination_code = 2;
                }
            }

        } else {

            a_termination_code = -3;
        }
    }

    if (b_termination_code == 0) {

        // Looks for a valid hedge corresponding to this edge

        int hedge = pointhedge(0, b_prev_pt, b_this_pt);

        if (hedge == -1) {

            hedge = pointhedge(0, b_this_pt, b_prev_pt);

            if (hedge == -1) {
                error(\"There is no edge between points %d and %d.\", b_prev_pt, b_this_pt);
                return;
            }
        }

        // Looks for a unique valid next edge to potentially select

        int hedge_equivcount = hedge_equivcount(0, hedge);

        if (hedge_equivcount < 3) {

            if (hedge_equivcount == 1) {
                if (hedge_presrcpoint(0, hedge) == -1) {
                    b_termination_code = -1;
                }
            }

            if (b_termination_code == 0) {

                // Builds the invalid point array

                int invalid_pts[] = {};
                append(invalid_pts, b_prev_pt);

                for (int i = 0; i < hedge_equivcount; i ++) {
                    append(invalid_pts, hedge_presrcpoint(0, hedge));
                    append(invalid_pts, hedge_postdstpoint(0, hedge));
                    hedge = hedge_nextequiv(0, hedge);
                }

                // Looks for valid next points from neighbour points

                int neighbour_pts[] = neighbours(0, b_this_pt);
                int count = 0;

                foreach (int neighbour_pt; neighbour_pts) {

                    if (find(invalid_pts, neighbour_pt) < 0) {
                        b_next_pt = neighbour_pt;
                        count ++;
                    }
                }

                // If this is a unique valid next point, selects
                // the corresponding edge according to the pattern

                if (count == 1) {

                    if (!apply_pattern) {

                        setedgegroup(0, group_name, b_this_pt, b_next_pt, 1);

                        if (create_group_per_loop) {
                            setedgegroup(0, group_name + itoa(loop_index), b_this_pt, b_next_pt, 1);
                        }

                    } else {

                        if ((-iteration + pattern_offset) % period < select_length) {

                            setedgegroup(0, group_name, b_this_pt, b_next_pt, 1);

                            if (create_group_per_loop) {
                                setedgegroup(0, group_name + itoa(loop_index), b_this_pt, b_next_pt, 1);
                            }
                        }
                    }

                    b_prev_pt = b_this_pt;
                    b_this_pt = b_next_pt;

                    if (b_this_pt == a_this_pt) {
                        a_termination_code = 1;
                        b_termination_code = 1;
                    }

                } else {

                    b_termination_code = 2;
                }
            }

        } else {

            b_termination_code = -3;
        }
    }

    if (iteration >= max_iteration) {
        a_termination_code = a_termination_code == 0 ? 10 : a_termination_code;
        b_termination_code = b_termination_code == 0 ? 10 : b_termination_code;
    }
}

// DEBUG ONLY
/*
i[]@code = {};
append(i[]@code, a_termination_code);
append(i[]@code, b_termination_code);
i[]@end_pts = {};
append(i[]@end_pts, a_this_pt);
append(i[]@end_pts, b_this_pt);
i@iterations = iteration + 1;
*/"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
max_iteration	[ 0	locks=0 ]	(	[ max_iteration	0 ] 	)
apply_pattern	[ 0	locks=0 ]	(	[ apply_pattern	0 ] 	)
pattern_offset	[ 0	locks=0 ]	(	[ pattern_offset	0 ] 	)
select_length	[ 0	locks=0 ]	(	[ select_length	0 ] 	)
