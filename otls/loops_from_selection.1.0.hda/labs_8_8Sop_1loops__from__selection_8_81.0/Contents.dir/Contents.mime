--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/select_quad_loop.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int max_iteration = chi(\"max_iteration\");

if (max_iteration < 1) {
    return;
}

int iteration = 0;
int start_pt0 = point(1, \"_lfs_index\", 0);
int start_pt1 = point(1, \"_lfs_index\", 1);

string group_name = chs(\"group_name\");
int create_group_per_loop = chi(\"create_group_per_loop\");
int loop_index = point(1, \"_lfs_loop_index\", 0);
int quads_only = chi(\"quads_only\");

int apply_pattern = chi(\"apply_pattern\");
int pattern_offset, select_length, skip_length, period;

if (apply_pattern) {

    pattern_offset = chi(\"pattern_offset\");
    select_length = chi(\"select_length\");
    skip_length = chi(\"skip_length\");
    period = select_length + skip_length;
}

// The two search directions are identified as a and b.
int a_this_hedge = -1;
int b_this_hedge = -1;

// Should keep going: 0
// The loop is complete: 1
// There is no next hedge: 2
// Terminated at a non-quad: 3
// Terminated before a non-quad: 4
// The max iteration is reached: 10
// There is no face attached to this edge: -1
// The mesh is non-manifold at this edge: -3
int a_termination_code = 0;
int b_termination_code = 0;

// For one of the directions, looks for a valid hedge corresponding to this edge

a_this_hedge = pointhedge(0, start_pt0, start_pt1);

if (a_this_hedge == -1) {

    a_this_hedge = pointhedge(0, start_pt1, start_pt0);

    if (a_this_hedge == -1) {
        error(\"There is no edge between points %d and %d.\", start_pt0, start_pt1);
        return;
    }
}

// If the start edge is valid, looks for a valid hedge for the other direction

int start_hedge_equivcount = hedge_equivcount(0, a_this_hedge);

if (start_hedge_equivcount == 2) {

    b_this_hedge = hedge_nextequiv(0, a_this_hedge);

} else if (start_hedge_equivcount == 1) {

    if (hedge_presrcpoint(0, a_this_hedge) != -1) {
        b_termination_code = 2;

    } else {
        a_termination_code = -1;
        b_termination_code = -1;
    }

} else {

    a_termination_code = -3;
    b_termination_code = -3;
}

// Attempts to complete the loop from both directions
while (a_termination_code == 0 || b_termination_code == 0) {

    if (a_termination_code == 0) {

        // Selects this prim according to the termination rule and the pattern.
        // If this prim is a quad, attempts to find the next hedge.

        int prim = hedge_prim(0, a_this_hedge);

        if (primvertexcount(0, prim) == 4) {
            // This prim is a quad:

            if (!apply_pattern) {

                setprimgroup(0, group_name, prim, 1);

                if (create_group_per_loop) {
                    setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                }

            } else {

                if ((iteration + pattern_offset) % period < select_length) {

                    setprimgroup(0, group_name, prim, 1);

                    if (create_group_per_loop) {
                        setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                    }
                }
            }

            a_this_hedge = hedge_next(0, hedge_next(0, a_this_hedge));

            if (a_this_hedge != b_this_hedge) {

                int hedge_equivcount = hedge_equivcount(0, a_this_hedge);

                if (hedge_equivcount == 2) {
                    a_this_hedge = hedge_nextequiv(0, a_this_hedge);

                } else if (hedge_equivcount == 1) {
                    a_termination_code = 2;

                } else {
                    a_termination_code = -3;
                }

            } else {

                a_termination_code = 1;
                b_termination_code = 1;
            }

        } else {
            // This prim is not a quad:

            if (!quads_only) {

                if (!apply_pattern) {

                    setprimgroup(0, group_name, prim, 1);

                    if (create_group_per_loop) {
                        setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                    }

                } else {

                    if ((iteration + pattern_offset) % period < select_length) {

                        setprimgroup(0, group_name, prim, 1);

                        if (create_group_per_loop) {
                            setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                        }
                    }
                }

                a_termination_code = 3;

            } else {

                a_termination_code = 4;
            }
        }
    }

    if (b_termination_code == 0) {

        // Selects this prim according to the termination rule and the pattern.
        // If this prim is a quad, attempts to find the next hedge.

        int prim = hedge_prim(0, b_this_hedge);

        if (primvertexcount(0, prim) == 4) {
            // This prim is a quad:

            if (!apply_pattern) {

                setprimgroup(0, group_name, prim, 1);

                if (create_group_per_loop) {
                    setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                }

            } else {

                if ((-iteration - 1 + pattern_offset) % period < select_length) {

                    setprimgroup(0, group_name, prim, 1);

                    if (create_group_per_loop) {
                        setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                    }
                }
            }

            b_this_hedge = hedge_next(0, hedge_next(0, b_this_hedge));

            if (b_this_hedge != a_this_hedge) {

                int hedge_equivcount = hedge_equivcount(0, b_this_hedge);

                if (hedge_equivcount == 2) {
                    b_this_hedge = hedge_nextequiv(0, b_this_hedge);

                } else if (hedge_equivcount == 1) {
                    b_termination_code = 2;

                } else {
                    b_termination_code = -3;
                }

            } else {

                a_termination_code = 1;
                b_termination_code = 1;
            }

        } else {
            // This prim is not a quad:

            if (!quads_only) {

                if (!apply_pattern) {

                    setprimgroup(0, group_name, prim, 1);

                    if (create_group_per_loop) {
                        setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                    }

                } else {

                    if ((-iteration - 1 + pattern_offset) % period < select_length) {

                        setprimgroup(0, group_name, prim, 1);

                        if (create_group_per_loop) {
                            setprimgroup(0, group_name + itoa(loop_index), prim, 1);
                        }
                    }
                }

                b_termination_code = 3;

            } else {

                b_termination_code = 4;
            }
        }
    }

    iteration ++;

    if (iteration >= max_iteration) {
        a_termination_code = a_termination_code == 0 ? 10 : a_termination_code;
        b_termination_code = b_termination_code == 0 ? 10 : b_termination_code;
    }
}

// DEBUG ONLY
/*
i[]@code = {};
append(i[]@code, a_termination_code);
append(i[]@code, b_termination_code);
i[]@hedges = {};
append(i[]@hedges, a_this_hedge);
append(i[]@hedges, b_this_hedge);
i@iterations = iteration;
*/"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
max_iteration	[ 0	locks=0 ]	(	[ max_iteration	0 ] 	)
apply_pattern	[ 0	locks=0 ]	(	[ apply_pattern	0 ] 	)
pattern_offset	[ 0	locks=0 ]	(	[ pattern_offset	0 ] 	)
select_length	[ 0	locks=0 ]	(	[ select_length	0 ] 	)
skip_length	[ 0	locks=0 ]	(	[ skip_length	0 ] 	)
create_group_per_loop	[ 0	locks=0 ]	(	[ create_group_per_loop	0 ] 	)
quads_only	[ 0	locks=0 ]	(	[ quads_only	0 ] 	)
group_name	[ 0	locks=0 ]	(	[ group_name	"" ] 	)
}

